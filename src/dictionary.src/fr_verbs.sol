 paradigm déplacer : FR_VERB for "(.+)cer"
 {
  FR_VERB_FORM:INFINITIVE { "" } // déplacer

  // indicatif & présent
  FR_VERB_FORM:PRESENT |{
                         FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-1" }        // je déplace
                         FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-3%+cons" }  // nous déplacons
						 
                         FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-1%+s" }     // tu déplaces
                         FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-1%+z" }     // vous déplacez
						 
                         FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-1" }        // il déplace
                         FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-1%+nt" }    // ils déplacent
                        }

  // indicatif & imparfait
  FR_VERB_FORM:IMPERFECT |{
                           FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-3%+çais" }  // je   déplaçais 
                           FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-2%+ions" }  // nous déplacions
						 
                           FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-3%+çais" }  // tu   déplaçais
                           FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-2%+iez" }   // vous déplaciez
						 
                           FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-3%+çait" }   // il/elle/on déplaçait 
                           FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-3%+çaient" } // ils/elles déplaçaient
                          }

  // indicatif & passé simple
  FR_VERB_FORM:SIMPLE_PAST |{
                             FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-3%+çai" }     // je   déplaçai
                             FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-3%+çâmes" }   // nous déplaçâmes 
						 
                             FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-3%+ças" }     // tu  	déplaças 
                             FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-3%+çâtes" }   // vous  déplaçâtes 
						 
                             FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-3%+ça" }      // il/elle/on  	déplaça  
                             FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-3%+cèrent" }  // ils/elles  	déplacèrent 
                            }

  // indicatif & futur
  FR_VERB_FORM:FUTURE |{
                        FR_PERSON:1 FR_NOMBRE:SINGULIER { "%+ai" }   // je déplacerai
                        FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%+ons" }  // nous déplacerons
						 
                        FR_PERSON:2 FR_NOMBRE:SINGULIER { "%+as" }   // tu déplaceras
                        FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%+ez" }   // vous déplacerez
						 
                        FR_PERSON:3 FR_NOMBRE:SINGULIER { "%+a" }    // il déplacera
                        FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%+ont" }  // ils déplaceront
                       }

  // impératif
  FR_VERB_FORM:IMPERATIVE |{
                            FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-3%+çons" }  // nous  déplaçons
                            FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-2%+e" }    // tu  	 déplace 
                            FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-2%+ez" }   // vous  déplacez 
                           }

  // subjonctif 	présent
  FR_VERB_FORM:SUBJUNCTIVE_PRESENT |{
                                     FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-1" }       // déplace
                                     FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-2%+ions" } // déplacions
						 
                                     FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-1%+s" }    // déplaces
                                     FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-2%+iez" }  // déplaciez
						 
                                     FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-1" }       // déplace
                                     FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-1%+nt" }   // déplacent
                                    }

  // subjonctif 	imparfait
