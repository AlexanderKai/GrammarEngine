 paradigm finir : FR_VERB  for "(.+)ir"
 {
  FR_VERB_FORM:INFINITIVE { "" } // finir
  FR_VERB_FORM:PRESENT |{
                         FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-1%+s" }      // je finis
                         FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-1%+ssons" }  // nous finissons
						 
                         FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-1%+s" }      // tu finis
                         FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-1%+ssez" }   // vous finissez
						 
                         FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-1%+t" }      // il finit
                         FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-1%+ssent" }  // ils finissent
                        }

  FR_VERB_FORM:IMPERFECT |{
                           FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-1%+ssais" }   // je  	finissais 
                           FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-1%+ssions" }  // nous  	finissions
						 
                           FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-1%+ssais" }   // tu  	finissais 
                           FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-1%+ssiez" }   // vous  	finissiez
						 
                           FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-1%+ssait" }    // il/elle/on  	finissait
                           FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-1%+ssaient" }  // ils/elles  	finissaient 
                          }

  FR_VERB_FORM:FUTURE |{
                         FR_PERSON:1 FR_NOMBRE:SINGULIER { "%+ai" }   // je finirai
                         FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%+ons" }  // nous finirons
						 
                         FR_PERSON:2 FR_NOMBRE:SINGULIER { "%+as" }   // tu finiras
                         FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%+ez" }   // vous finirez
						 
                         FR_PERSON:3 FR_NOMBRE:SINGULIER { "%+a" }    // il finira
                         FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%+ont" }  // ils finiront
                        }


  FR_VERB_FORM:SIMPLE_PAST |{
                            FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-1%+s" }     // je  	finis 
                            FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-2%+îmes" }  //  nous  	finîmes
						 
                            FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-1%+s" }     // tu  	finis 
                            FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-2%+îtes" }  // vous  	finîtes 
						 
                            FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-1%+t" }     // il/elle/on  	finit 
                            FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%+ent" }      // ils/elles  	finirent
                           }
						   
  FR_VERB_FORM:IMPERATIVE |{
                            FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-3%+sissons" }  // nous  finissons
                            FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-3%+sis" }      // tu  	 finis
                            FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-3%+sissez" }   // vous  finissez
                           }

  // subjonctif 	présent
  FR_VERB_FORM:SUBJUNCTIVE_PRESENT |{
                                     FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-1%+sse" }    // finisse
                                     FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-1%+ssions" } // finissions
						 
                                     FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-1%+sses" }   // finisses
                                     FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-1%+ssiez" }  // finissiez
						 
                                     FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-1%+sse" }    // finisse
                                     FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-1%+ssent" }  // finissent
                                    }

  // subjonctif 	imparfait
  FR_VERB_FORM:SUBJUNCTIVE_IMPERFECT |{
                                       FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-1%+sse" }    // finisse
                                       FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-1%+ssions" } // finissions
						           
                                       FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-1%+sses" }   // finisses
                                       FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-1%+ssiez" }  // finissiez
						           
                                       FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-2%+ît" }      // finît
                                       FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-1%+ssent" }   // finissent
                                      }

  // conditionnel 	présent
  FR_VERB_FORM:CONDITIONAL |{
                             FR_PERSON:1 FR_NOMBRE:SINGULIER { "%+ais" }  // finirais
                             FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%+ions" } // finirions
						    
                             //FR_PERSON:2 FR_NOMBRE:SINGULIER { "%+ais" } // 
                             FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%+iez" }   // finiriez
						    
                             FR_PERSON:3 FR_NOMBRE:SINGULIER { "%+ait" }   // finirait
                             FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%+aient" } // finiraient
                            }

  // participe présent
  FR_VERB_FORM:PRESENT_PARTICIPLE { "%-1%+ssant" } // finissant

  // participe 	passé
  FR_VERB_FORM:PAST_PARTICIPLE |{
                                 FR_NOMBRE:SINGULIER FR_GENRE:MASCULINE { "%-1" }     // fini
                                 FR_NOMBRE:PLURIEL   FR_GENRE:MASCULINE { "%-1%+s" }  // finis
                                 FR_NOMBRE:SINGULIER FR_GENRE:FEMININE  { "%-1%+e" }  // finie
                                 FR_NOMBRE:PLURIEL   FR_GENRE:FEMININE  { "%-1%+es" } // finies
                                }

 }

 
