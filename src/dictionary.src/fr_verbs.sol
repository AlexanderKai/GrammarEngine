                                       FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-3%+çassions" } // déplaçassions
						           
                                       FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-3%+çasses" }   // déplaçasses
                                       FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-3%+çassiez" }  // déplaçassiez
						           
                                       FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-3%+çât" }      // déplaçât
                                       FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-3%+çassent" }  // déplaçassent
                                      }

  // conditionnel 	présent
  FR_VERB_FORM:CONDITIONAL |{
                             FR_PERSON:1 FR_NOMBRE:SINGULIER { "%+ais" }   // déplacerais
                             FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%+ions" }  // déplacerions
						    
                             FR_PERSON:2 FR_NOMBRE:SINGULIER { "%+ais" }   // déplacerais
                             FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%+iez" }   // déplaceriez
						    
