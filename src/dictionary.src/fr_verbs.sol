 paradigm appeler : FR_VERB for "(.+)eler"
 {
  FR_VERB_FORM:INFINITIVE { "" } // appeler
  
  FR_VERB_FORM:PRESENT |{
                         FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-2%+le" }   // je appelle
                         FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-2%+ons" }  // nous appelons
						 
                         FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-2%+les" }  // tu appelles
                         FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-1%+z" }    // vous appelez
						 
                         FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-2%+le" }   // il appelle
                         FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-2%+lent" } // ils appellent
                        }

  FR_VERB_FORM:IMPERFECT |{
                           FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-2%+ais" }   // j’ 	appelais 
                           FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-2%+ions" }  // nous  	appelions
						 
                           FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-2%+ais" }   // tu  	appelais
                           FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-2%+iez" }   // vous  	appeliez
						 
                           FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-2%+ait" }   // il/elle/on  	appelait 
                           FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-2%+aient" } // ils/elles  	appelaient 
                          }

  FR_VERB_FORM:FUTURE |{
                         FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-2%+lerai" }   // j’appellerai
                         FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-2%+lerons" }  // nous appellerons
						 
                         FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-2%+leras" }   // tu appelleras 
                         FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-2%+lerez" }   // vous appellerez 
						 
                         FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-2%+lera" }    // il/elle/on appellera 
                         FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-2%+leront" }  // ils/elles appelleront
                        }

  FR_VERB_FORM:SIMPLE_PAST |{
                             FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-2%+ai" }    // j’ 	appelai 
                             FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-2%+âmes" }  //  nous  	appelâmes
						  
                             FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-2%+as" }    // tu  	appelas
                             FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-2%+âtes" }  // vous  	appelâtes
						  
                             FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-2%+a" }     // il/elle/on  	appela   
                             FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-2%+èrent" } // ils/elles  	appelèrent 
                            }

  // subjonctif 	présent
  FR_VERB_FORM:SUBJUNCTIVE_PRESENT |{
                                     FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-2%+ler" }  // appelle
                                     FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-2%+ions" } // appelions
						 
                                     FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-2%+les" }  // appelles
                                     FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-2%+iez" }  // appeliez
						 
                                     FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-2%+le" }   // appelle
                                     FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-2%+lent" } // appellent
                                    }

  // subjonctif 	imparfait
  FR_VERB_FORM:SUBJUNCTIVE_IMPERFECT |{
                                       FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-2%+asse" }    // appelasse
                                       FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-2%+assions" } // appelassions
						           
                                       FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-2%+asses" }   // appelasses
                                       FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-2%+assiez" }  // appelassiez
						           
                                       FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-2%+ât" }      // appelât
                                       FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-2%+assent" }  // appelassent
                                      }

  // conditionnel 	présent
  FR_VERB_FORM:CONDITIONAL |{
                             FR_PERSON:1 FR_NOMBRE:SINGULIER { "%-2%+lerais" }   // appellerais
                             FR_PERSON:1 FR_NOMBRE:PLURIEL   { "%-2%+lerions" }  // appellerions
						    
                             FR_PERSON:2 FR_NOMBRE:SINGULIER { "%-2%+lerais" }   // appellerais
                             FR_PERSON:2 FR_NOMBRE:PLURIEL   { "%-2%+leriez" }   // appelleriez
						    
                             FR_PERSON:3 FR_NOMBRE:SINGULIER { "%-2%+lerait" }   // appellerait
                             FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%-2%+leraient" } // appelleraient
                            }



  // participe présent
  FR_VERB_FORM:PRESENT_PARTICIPLE { "%-2%+ant" } // appelant

  // participe 	passé
  FR_VERB_FORM:PAST_PARTICIPLE |{
                                 FR_NOMBRE:SINGULIER FR_GENRE:MASCULINE { "%-2%+é" }   // appelé
                                 FR_NOMBRE:PLURIEL   FR_GENRE:MASCULINE { "%-2%+és" }  // appelés
                                 FR_NOMBRE:SINGULIER FR_GENRE:FEMININE  { "%-2%+ée" }  // appelée
                                 FR_NOMBRE:PLURIEL   FR_GENRE:FEMININE  { "%-2%+ées" } // appelées
                                }

 }



