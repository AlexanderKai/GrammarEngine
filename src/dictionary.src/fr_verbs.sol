                             FR_PERSON:3 FR_NOMBRE:PLURIEL   { "%+aient" } // déplaceraient
                            }


  // participe présent
  FR_VERB_FORM:PRESENT_PARTICIPLE { "%-3%+çant" } // déplaçant

  // participe 	passé
  FR_VERB_FORM:PAST_PARTICIPLE |{
                                 FR_NOMBRE:SINGULIER FR_GENRE:MASCULINE { "%-2%+é" }   // déplacé
