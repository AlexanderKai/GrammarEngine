#include "sg_defs.h"

automat sg
{
 entry ОДИН : ЧИСЛИТЕЛЬНОЕ
 {
  КАТЕГОРИЯ_ЧИСЛ:КОЛИЧ
  ПАДЕЖ:(ИМ) |{
               ЧИСЛО:ЕД РОД { ОДИН ОДНА ОДНО }
               ЧИСЛО:МН     { ОДНИ }
              }

  ПАДЕЖ:(РОД) |{
                ЧИСЛО:ЕД РОД { ОДНОГО ОДНОЙ ОДНОГО }
                ЧИСЛО:МН     { ОДНИХ }
               }

  ПАДЕЖ:ТВОР |{
               ЧИСЛО:ЕД РОД     { ОДНИМ ОДНОЙ ОДНИМ }
               ЧИСЛО:ЕД РОД:ЖЕН {       ОДНОЮ       }
               ЧИСЛО:МН         { ОДНИМИ            }
              }

  ПАДЕЖ:ВИН |{
              ЧИСЛО:ЕД РОД:МУЖ ОДУШ { ОДНОГО ОДИН }
              ЧИСЛО:ЕД РОД:ЖЕН      { ОДНУ        }
              ЧИСЛО:ЕД РОД:СР       { ОДНО        }
              ЧИСЛО:МН ОДУШ         { ОДНИХ ОДНИ  }
             }

  ПАДЕЖ:ДАТ |{
              ЧИСЛО:ЕД РОД { ОДНОМУ ОДНОЙ ОДНОМУ }
              ЧИСЛО:МН     { ОДНИМ }
             }

  ПАДЕЖ:(ПРЕДЛ) |{
                  ЧИСЛО:ЕД РОД { ОДНОМ ОДНОЙ ОДНОМ }
                  ЧИСЛО:МН     { ОДНИХ }
                 }
 }


 entry ПОЛТОРА : ЧИСЛИТЕЛЬНОЕ
 {
  КАТЕГОРИЯ_ЧИСЛ:КОЛИЧ
  ПАДЕЖ:(ИМ)  ЧИСЛО:ЕД РОД { ПОЛТОРА ПОЛТОРЫ ПОЛТОРА }
  ПАДЕЖ:(РОД) ЧИСЛО:ЕД РОД { ПОЛУТОРА ПОЛУТОРА ПОЛУТОРА }
  ПАДЕЖ:ТВОР  ЧИСЛО:ЕД РОД  { ПОЛУТОРАМИ ПОЛУТОРАМИ ПОЛУТОРАМИ }
  ПАДЕЖ:ВИН |{
              ЧИСЛО:ЕД РОД:МУЖ ОДУШ { ПОЛТОРА ПОЛТОРА }
              ЧИСЛО:ЕД РОД:ЖЕН { ПОЛТОРЫ         }
              ЧИСЛО:ЕД РОД:СР  { ПОЛТОРА         }
             }

  ПАДЕЖ:ДАТ     ЧИСЛО:ЕД РОД { ПОЛУТОРАМ ПОЛУТОРАМ ПОЛУТОРАМ }
  ПАДЕЖ:(ПРЕДЛ) ЧИСЛО:ЕД РОД { ПОЛУТОРАХ ПОЛУТОРАХ ПОЛУТОРАХ }
 }


