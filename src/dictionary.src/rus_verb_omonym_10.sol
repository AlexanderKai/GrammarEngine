 link Глагол : кликать { ВИД:НЕСОВЕРШ ПАДЕЖ:Вин }   <в_инфинитив> ИНФИНИТИВ : кликать { ВИД:НЕСОВЕРШ ПАДЕЖ:Вин }
 link ИНФИНИТИВ : кликать { ВИД:НЕСОВЕРШ ПАДЕЖ:Вин } <в_предикат> Глагол : кликать { ВИД:НЕСОВЕРШ ПАДЕЖ:Вин }

 link Глагол : кликать { ВИД:НЕСОВЕРШ ПАДЕЖ:ТВОР }  <в_инфинитив> ИНФИНИТИВ : кликать { ВИД:НЕСОВЕРШ ПАДЕЖ:ТВОР }
 link ИНФИНИТИВ : кликать { ВИД:НЕСОВЕРШ ПАДЕЖ:ТВОР } <в_предикат> Глагол : кликать { ВИД:НЕСОВЕРШ ПАДЕЖ:ТВОР } 



 link Глагол : кликнуть { ВИД:НЕСОВЕРШ ПАДЕЖ:Вин }   <в_инфинитив> ИНФИНИТИВ : кликнуть { ВИД:НЕСОВЕРШ ПАДЕЖ:Вин }
 link ИНФИНИТИВ : кликнуть { ВИД:НЕСОВЕРШ ПАДЕЖ:Вин } <в_предикат> Глагол : кликнуть { ВИД:НЕСОВЕРШ ПАДЕЖ:Вин }

 link Глагол : кликнуть { ВИД:НЕСОВЕРШ ПАДЕЖ:ТВОР }  <в_инфинитив> ИНФИНИТИВ : кликнуть { ВИД:НЕСОВЕРШ ПАДЕЖ:ТВОР }
 link ИНФИНИТИВ : кликнуть { ВИД:НЕСОВЕРШ ПАДЕЖ:ТВОР } <в_предикат> Глагол : кликнуть { ВИД:НЕСОВЕРШ ПАДЕЖ:ТВОР } 

 link ИНФИНИТИВ : кликнуть { ВИД:НЕСОВЕРШ ПАДЕЖ:ТВОР } <в_сов> ИНФИНИТИВ : кликать { ВИД:НЕСОВЕРШ ПАДЕЖ:ТВОР }

 omonym remove Глагол:кликнуть{}
 omonym remove Инфинитив:кликнуть{}
 omonym remove Глагол:кликать{}
 omonym remove Инфинитив:кликать{}

 // ----------------------------------

 omonym add ПРИЛАГАТЕЛЬНОЕ : кликавший{}
 omonym add ПРИЛАГАТЕЛЬНОЕ : кликающий{}

 entry кликавший : ПРИЛАГАТЕЛЬНОЕ
 {
  paradigm Прил_5034
  ПРИЧАСТИЕ
  ВРЕМЯ:ПРОШЕДШЕЕ
  ПЕРЕХОДНОСТЬ:ПЕРЕХОДНЫЙ
  ПАДЕЖВАЛ:ВИН
 }

 entry кликающий : ПРИЛАГАТЕЛЬНОЕ
 {
  paradigm Прил_5034
  ПРИЧАСТИЕ
  ВРЕМЯ:НАСТОЯЩЕЕ
  ПЕРЕХОДНОСТЬ:ПЕРЕХОДНЫЙ
  ПАДЕЖВАЛ:ВИН
 }

 entry кликавший : ПРИЛАГАТЕЛЬНОЕ
 {
  paradigm Прил_5034
  ПРИЧАСТИЕ
  ВРЕМЯ:ПРОШЕДШЕЕ
  ПЕРЕХОДНОСТЬ:ПЕРЕХОДНЫЙ
  ПАДЕЖВАЛ:ТВОР
 }

